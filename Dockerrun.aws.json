{
    "AWSEBDockerrunVersion": "2",
    "volumes": [
        {
            "name": "asf-config",
            "host": {
                "sourcePath": "/opt/asf/config"
            }
        }
    ],
    "containerDefinitions": [
        {
            "name": "asf",
            "image": "jat001/archisteamfarm:0.0.2",
            "essential": true,
            "mountPoints": [
                {
                    "sourceVolume": "asf-config",
                    "containerPath": "/opt/asf/config"
                }
            ]
        }
    ]
}